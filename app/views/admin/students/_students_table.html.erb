<h2>Students</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Matches</th>
    </tr>
  </thead>
  <tbody>
    <% students.each do |student| %>
      <tr>
        <td><%= student.name %></td>
        <td><%= student.matches %></td>
      </tr>
    <% end %>
  </tbody>
</table>
